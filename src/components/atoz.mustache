<div class="atoz-nav">
  <ul>
  {{#letters}}
    <li><a href="#{{this.letter}}">{{this.letter}}</a></li>
  {{/letters}}
  </ul>
</div>
<div class="atoz-container">
  <ul>
  {{#letters}}
    <li id="{{this.letter}}" class="atoz-letter">
      <h5>{{this.letter}}</h5>
      <div class="atoz-content">
        <ul>
        {{#this.pages}}
          <li class="atoz-page">
            <a href="{{this.link}}">
              <h6>{{this.title}}</h6>
              {{#this.content}}{{this}}{{/this.content}}
            </a>
          </li>
        {{/this.pages}}
        </ul>
      </div>
    </li>
  {{/letters}}
  </ul>
</div>
